<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uk.co.androidalliance.edgeeffectoverride.sample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo" >
        <activity
            android:name="uk.co.androidalliance.edgeeffectoverride.sample.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="uk.co.androidalliance.edgeeffectoverride.sample.ListViewActivity"
            android:label="@string/title_activity_list_view" />
        <activity
            android:name="uk.co.androidalliance.edgeeffectoverride.sample.GridViewActivity"
            android:label="@string/title_activity_grid_view" >
        </activity>
        <activity
            android:name="uk.co.androidalliance.edgeeffectoverride.sample.ExpandableListViewActivity"
            android:label="@string/title_activity_expandable_list_view" >
        </activity>
        <activity
            android:name="uk.co.androidalliance.edgeeffectoverride.sample.ScrollViewActivity"
            android:label="@string/title_activity_scroll_view" >
        </activity>
        <activity
            android:name="uk.co.androidalliance.edgeeffectoverride.sample.ViewPagerActivity"
            android:label="@string/title_activity_view_pager" >
        </activity>
        <activity
                android:name="uk.co.androidalliance.edgeeffectoverride.sample.WebViewActivity"
                android:label="@string/title_activity_web_view" >
        </activity>
    </application>

</manifest>
